<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>My App</display-name>
  <filter>
  	<filter-name>CORSFilter</filter-name>
  	<filter-class>com.skaas.webapp.CORSFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CORSFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>Helloservlet</servlet-name>
  	<display-name>Helloservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Helloservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Loginservlet</servlet-name>
  	<display-name>Loginservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Loginservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Signupservlet</servlet-name>
  	<display-name>Signupservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Signupservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Helloservlet</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Loginservlet</servlet-name>
  	<url-pattern>/token</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Signupservlet</servlet-name>
  	<url-pattern>/account</url-pattern>
  </servlet-mapping>
</web-app>
